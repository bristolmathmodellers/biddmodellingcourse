file = {:Users/sa15093-l/Dropbox/Papers/Bhatti et al. - 1995 - Increasing incidence of tuberculosis in England and Wales a study of the likely causes.pdf:pdf},
isbn = {0959-8138},
issn = {0959-8138},
journal = {BMJ},
number = {6985},
pages = {967--969},
