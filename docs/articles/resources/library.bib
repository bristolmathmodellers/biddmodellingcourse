journal = {Public Health},
keywords = {Bacillus Camette-Guerin (BCG) vaccine,Incidences rates,Neonates,Selective immunization,Tuberculosis,Universal immunization},
number = {3},
pages = {179--183},
pmid = {8668765},
title = {{Selective or universal neonatal BCG immunization: What policy for a district with a high incidence of tuberculosis?}},
volume = {110},
year = {1996}
