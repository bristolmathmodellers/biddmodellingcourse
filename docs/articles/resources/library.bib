journal = {Emerg. Infect. Dis.},
keywords = {a population,imicrobial use and antimicrobial,resistance},
number = {4},
pages = {347--354},
pmid = {11971765},
title = {{Antimicrobial use and antimicrobial resistance: A population perspective}},
volume = {8},
year = {2002}
}
