journal = {{\ldots} Epidemiol. Community Heal.},
keywords = {Causality,Confounding Factors (Epidemiology),Data Interpretation,Effect Modifier,Epidemiologic,Epidemiologic Research Design,Epidemiologic Studies,Humans,Probability,Research Design,Statistical},
number = {7},
pages = {578--86},
pmid = {16790829},
title = {{Estimating causal effects from epidemiological data}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2652882{\&}tool=pmcentrez{\&}rendertype=abstract{\%}5Cnhttp://jech.bmj.com/content/60/7/578.short},
volume = {60},
