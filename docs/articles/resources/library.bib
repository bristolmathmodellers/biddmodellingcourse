isbn = {1664-302X},
issn = {1664302X},
journal = {Front. Microbiol.},
keywords = {Antimicrobial resistance,Environment,Human and veterinary medicine,Resistance genes,Soil,Wastewater},
number = {MAY},
pages = {1--14},
pmid = {23675371},
title = {{A brief multi-disciplinary review on antimicrobial resistance in medicine and its linkage to the global environmental microbiota}},
