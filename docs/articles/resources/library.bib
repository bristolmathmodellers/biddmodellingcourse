@article{Roeger2009,
abstract = {Tuberculosis (TB) is the leading cause of death among individuals infected with the human immunodeficiency virus (HIV). The study of the joint dynamics of HIV and TB present formidable mathematical challenges due to the fact that the models of transmission are quite distinct. Furthermore, although there is overlap in the populations at risk of HIV and TB infections, the magnitude of the proportion of individuals at risk for both diseases is not known. Here, we consider a highly simplified deterministic model that incorporates the joint dynamics of TB and HIV, a model that is quite hard to analyze. We compute independent reproductive numbers for TB (R1) and HIV (R2) and the overall reproductive number for the system, R=max{\{}R1, R2{\}}. The focus is naturally (given the highly simplified nature of the framework) on the qualitative analysis of this model. We find that if R {\textless} 1 then the disease-free equilibrium is locally asymptotically stable. The TB-only equilibrium ET is locally asymptotically stable if R1 {\textless} 1 and R2 {\textless} 1. However, the symmetric condition, R1 {\textless} 1 and R2 {\textgreater} 1, does not necessarily guarantee the stability of the HIV-only equilibrium EH, and it is possible that TB can coexist with HIV when R2 {\textgreater} 1. In other words, in the case when R1 {\textless} 1 and R2 {\textgreater} 1 (or when R1 {\textgreater} 1 and R2 {\textgreater} 1), we are able to find a stable HIV/TB coexistence equilibrium. Moreover, we show that the prevalence level of TB increases with R2 {\textgreater} 1 under certain conditions. Through simulations, we find that i) the increased progression rate from latent to active TB in co-infected individuals may play a significant role in the rising prevalence of TB; and ii) the increased progression rates from HIV to AIDS have not only increased the prevalence level of HIV while decreasing TB prevalence, but also generated damped oscillations in the system.},
author = {Roeger, Lih-Ing W and Feng, Zhilan and Castillo-Chavez, Carlos},
doi = {10.3934/mbe.2009.6.815},
file = {:Users/sa15093-l/Dropbox/Papers/Roeger, Feng, Castillo-Chavez - 2009 - Modeling TB and HIV co-infections.pdf:pdf},
isbn = {1547-1063},
issn = {1551-0018},
journal = {Math. Biosci. Eng.},
keywords = {and phrases,co-infection,disease reproduction number,grant dms-0719697,hiv,partially supported by nsf,tb,the second author is,tuberculosis},
number = {4},
pages = {815--837},
pmid = {19835430},
title = {{Modeling TB and HIV co-infections.}},
volume = {6},
year = {2009}
}
