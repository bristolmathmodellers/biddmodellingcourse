file = {:Users/sa15093-l/Dropbox/Papers/Baguelin et al. - 2010 - Vaccination against pandemic influenza AH1N1v in England A real-time economic evaluation.pdf:pdf},
isbn = {1873-2518 (Electronic)$\backslash$n0264-410X (Linking)},
issn = {0264410X},
journal = {Vaccine},
keywords = {Influenza,Mathematical Model,Vaccination},
