issn = {0950-2688},
journal = {Epidemiol. Infect.},
keywords = {Adolescent,Adult,Age Distribution,Age of Onset,Aged,Child,Child, Preschool,Disease Progression,England,England: epidemiology,Humans,Incidence,Infant,Male,Middle Aged,Models, Statistical,Morbidity,Population Surveillance,Recurrence,Risk Factors,Sensitivity and Specificity,Time Factors,Tuberculosis, Pulmonary,Tuberculosis, Pulmonary: epidemiology,Tuberculosis, Pulmonary: transmission,Wales,Wales: epidemiology},
