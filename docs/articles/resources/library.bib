issn = {0003-9888},
journal = {Arch. Dis. Child.},
pages = {1017--1021},
pmid = {18562450},
title = {{Epidemiology and treatment outcome of childhood tuberculosis in England and Wales: 1999-2006.}},
volume = {93},
year = {2008}
}
