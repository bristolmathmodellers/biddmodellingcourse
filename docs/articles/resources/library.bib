issn = {2045-2322},
journal = {Sci. Rep.},
pages = {4856},
pmid = {24798322},
title = {{Analytical modelling of the spread of disease in confined and crowded spaces.}},
url = {https://encore.lib.warwick.ac.uk/iii/encore/eds/C{\_}{\_}S infectious diseases of poverty{\_}{\_}Orightresult{\_}{\_}U{\_}{\_}X0?lang=eng{\&}link=http://0-search.ebscohost.com.pugwash.lib.warwick.ac.uk/login.aspx?direct=true{\&}site=eds-live{\&}db=edselc{\&}AN=edselc},
