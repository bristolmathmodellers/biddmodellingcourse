number = {NovemberMather, a. E., Matthews, L., Mellor, D. J., Reeve, R., Denwood, M. J., Boerlin, P., {\ldots} Reid, S. W. J. (2012). An ecological approach to assessing the epidemiology of antimicrobial resistance in animal and human populations, (November), 1630–1639. h},
pages = {1630--1639},
