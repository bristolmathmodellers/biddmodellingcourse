keywords = {Bacille Calmette-Guerin,Mathematical model,Pre-exposure and post-exposure vaccines,Reproduction number,Stability,Tuberculosis},
number = {3},
pages = {633--649},
pmid = {18644386},
title = {{Modelling the effects of pre-exposure and post-exposure vaccines in tuberculosis control}},
volume = {254},
year = {2008}
}
