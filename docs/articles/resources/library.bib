journal = {Theor Popul Biol},
keywords = {Virus Diseases/*epidemiology},
number = {October},
pages = {59--71},
pmid = {11589638},
title = {{Realistic distributions of infectious periods in epidemic models: changing patterns of persistence and dynamics.}},
volume = {60},
year = {2001}
