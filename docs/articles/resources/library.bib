keywords = {Adjuvants, Immunologic,Adolescent,Adult,Age Distribution,BCG Vaccine,CD4 Lymphocyte Count,Cicatrix,Drug Resistance, Bacterial,Female,HIV,HIV: isolation {\&} purification,Humans,Male,Middle Aged,Mycobacterium tuberculosis,Mycobacterium tuberculosis: isolation {\&} purificati,Prospective Studies,Sex Distribution,Sputum,Sputum: microbiology,Tuberculosis, Pulmonary,Tuberculosis, Pulmonary: drug therapy,Tuberculosis, Pulmonary: microbiology,Tuberculosis, Pulmonary: prevention {\&} control,Young Adult},
