isbn = {1080-6040 (Print)$\backslash$r1080-6040 (Linking)},
issn = {10806040},
journal = {Emerg. Infect. Dis.},
keywords = {earch,extensively drug-resistant,worldwide emergence of},
number = {3},
pages = {380--387},
