issn = {20952236},
journal = {Front. Comput. Sci.},
keywords = {agent-based models,complex networks,epidemicmodeling,human dynamics,infectious diseases,mathematics},
number = {5},
pages = {806--826},
title = {{Mathematical and computational approaches to epidemic modeling: a comprehensive review}},
