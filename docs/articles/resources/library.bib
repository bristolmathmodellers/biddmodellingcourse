file = {:Users/sa15093-l/Dropbox/Papers/Birch et al. - 2014 - Ten years of tuberculosis intervention in Greenland - Has it prevented cases of childhood tuberculosis.pdf:pdf},
issn = {22423982},
journal = {Int. J. Circumpolar Health},
keywords = {Children,Epidemiological study,Greenland,Tuberculosis control,Tuberculosis disease},
pages = {2--6},
